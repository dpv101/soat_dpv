<p id="notice"><%= notice %></p>

<h1>Listing Venta</h1>

<table class="table">
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Vehiculo</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venta.each do |venta| %>
      <tr>
        <td><%= venta.cliente.nombre_completo %></td>
        <td><%= venta.vehiculo.placa %></td>
        <td><%= venta.total %></td>
        <td><%= link_to "Ver detalle",  soat_path(venta.vehiculo.soat_active[0]) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
